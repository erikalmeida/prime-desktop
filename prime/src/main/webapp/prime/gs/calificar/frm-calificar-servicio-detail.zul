			</grid>
			<div id="divMenuItemAtencion" align="center" height="50px">	
				<hlayout style="padding: 0px">
					<button sclass="btn-primary btn-mini" zclass="btn"
						label="Calificar" id="btnAtencion"/>
						</hlayout>
						</div>
		</groupbox>
		
		
		
	</div>
		
			
			
			<div>
				<space />
			</div>
			<div>
				<space />
			</div>
		</div>
</div>