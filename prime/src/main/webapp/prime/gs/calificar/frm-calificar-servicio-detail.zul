							</listitem>

							

						</listbox>
					</row>

				</rows>
			</grid>
			<div id="divMenuItemInstalacion" align="center" height="50px">	
				<hlayout style="padding: 0px">
					<button sclass="btn-primary btn-mini" zclass="btn"
						label="Calificar" id="btnInstalacion"/>
						</hlayout>
						</div>
		
		</groupbox>
