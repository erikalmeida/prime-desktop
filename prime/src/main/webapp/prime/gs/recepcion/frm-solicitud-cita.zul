					<grid>
						<columns>
							<column width="30%" />
							<column width="10%" />
							<column width="10%" />
							<column width="10%" />
							<column width="10%" />
						</columns>
						<rows>
							<row zclass="form-group" class="text-center">
								<cell></cell>
								<button id="btn_registrar" type="button"
									width="" class="btn btn-primary">
									Registrar
								</button>
								
								<button id="btn_actualizar" type="button"
									width="" class="btn btn-primary">
									Actualizar
								</button>
								<button id="btn_volver" type="button"
									width="" class="btn btn-success">
									Volver
								</button>
								<button id="btn_cancelar" type="button"
