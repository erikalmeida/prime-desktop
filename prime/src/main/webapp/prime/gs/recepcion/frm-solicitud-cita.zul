<div xmlns:zk="zk" xmlns:z="zul"
	apply="">
	<div sclass="breadcrumb">
		<a href="#" tooltip="btntip,after_center"
			iconSclass="icon-home">
			Gestion de Agendas y Citas
			<custom-attributes title="Go to Home" />
		</a>
		<a href="#">Solicitud de Citas</a>
	</div>
	<vlayout xmlns:n="native" style="padding: 20px">
		<panel border="normal">
			<caption>
				<div style="margin-left: 40%;">
					Solicitud de Cita
				</div>
			</caption>
			<panelchildren>
				<div class="row-fluid">
					<grid>
						<columns>
							<column width="10%" />
							<column width="30%" />
							<column width="60%" />
						</columns>
						<rows>
							<row zclass="form-group">
								<cell></ceoll>
								<label style="padding-left: 20px;">
									Nombre:
								</label>
								<textbox id="txt_nombre"
									zclass="form-control" width="" />
									
									
							</row>
							
							
							<row zclass="form-group">
								<cell></cell>
								<label style="padding-left: 20px;">
									Apellido:
								</label>
								<textbox id="txt_apellido"
									zclass="form-control" width="" />
									
									
							</row>
							<row zclass="form-group">
								<cell></cell>
								<label style="padding-left: 20px;">
									Telefono:
								</label>
								<textbox id="txt_telefono"
									type="text" zclass="form-control" width="" />
							</row>
							<row zclass="form-group">
								<cell></cell>
								<label style="padding-left: 20px;">
									Correo:
								</label>
								<textbox id="txt_correo"
									zclass="form-control" width="" />
							</row>
							

						</rows>
					</grid>
					
					<grid>
					
					
					
        <auxhead>
            <auxheader colspan="5" label="Datos del vehiculo" />
        </auxhead>
        <columns>
        	<column hflex="4">Elegir</column>
            <column hflex="5">Marca</column>
            <column hflex="6">Modelo</column>
            <column hflex="3">Color</column>
            <column hflex="4">Año</column>
        </columns>
        <rows sclass="narrow">
            <row>
             <checkbox />
                <label value="Hyundai" />
                <label value="Sonata" />
                <label value="negro" />
                 <label value="2017" />
                <textbox />
            </row>
            <row>
              <checkbox />
                        <label value="Hyundai" />
                <label value="Elantra" />
                <label value="blanco" />
                 <label value="2017" />
                <textbox />
            </row>
            <row>
            <checkbox />
                       <label value="Chevrolet" />
                <label value="Aveo" />
                <label value="Azul" />
                 <label value="2015" />
                <textbox />
            </row>
            
            <row>
            <checkbox />
                      <label value="Ford" />
                <label value="Fiesta" />
                <label value="rojo" />
                 <label value="2016" />
                <textbox />
            </row>
            
        </rows>
        
       
    </grid>
    
    <vbox>
<hbox>
Breve descripcion de la falla: <textbox tabbable="true" rows="50"/>
</hbox>

</vbox>
    
    
  
    
    
    
