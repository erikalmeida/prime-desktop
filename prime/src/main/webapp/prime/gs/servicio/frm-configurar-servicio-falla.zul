											</menupopup>
										</combobutton>
									</div>
								</row>
							</rows>
						</grid>
						<space />
						<space />
					</groupbox>
				</div>

				<div align="center">
					<div align="center" style="max-width: 950px;">
						<listbox id="listTecnologias"
							nonselectableTags="*" oddRowSclass="none"
							emptyMessage="No se han encontrado resultados para esta búsqueda">
							<listhead>

								<listheader align="left"
									label="Seleccione la(s) Tecnologia(s)" width="56%">

								</listheader>


								<listheader align="left"
									label="Descripción" width="56%">
									<checkbox />
								</listheader>


							</listhead>
							<template name="model">


								<listitem>

									<listcell></listcell>

									<listcell
										style="text-align:left;">
										<label
											if="${each.estatus=='Inactivo'}" style="color: red"
											value="${each.descripcion}" />
										<label
											if="${each.estatus=='Activo'}" value="${each.descripcion}" />
									</listcell>

								</listitem>

							</template>
						</listbox>
						<space></space>
						<space></space>
						<div id="divMenuTecnologia" align="center"
							height="50px">
							<hlayout style="padding: 20px">
								<button sclass="btn-primary btn-mini"
									zclass="btn" label="Añadir" id="btnAnnadirTecnologia" />
								<button sclass="btn-danger btn-mini"
									zclass="btn" label="Volver" id="btnVolverTecnologia" />

							</hlayout>
						</div>
						<space></space>
						<space></space>
					</div>
				</div>
			</tabpanel>

			<tabpanel>

				<div align="center">
					<groupbox mold="3d" style="max-width: 950px;">
						<caption label="Etapa" />
						<grid style="border:none;">
							<columns>
								<column width="100%" />
							</columns>
							<rows>
								<row
									style="background:#FFFFFF;border:none;padding-top:20px;">
									<div align="center">
										<textbox id="txtBuscarEtapa"
											placeholder="Introduzca su búsqueda" width="60%"
											maxlength="50" />
										<combobutton label="Acciones">
											<menupopup>
												<menuitem
													iconSclass="z-icon-search" id="mItemBuscarEtapa"
													label="Buscar" sclass="mItem" />
												<menuitem
													iconSclass="z-icon-pencil-square-o" id="mItemIncluirEtapa"
													label="Incluir" sclass="mItem" visible="true" />
												<menuitem
													iconSclass="z-icon-pencil-square-o"
													id="mItemAsignacionEtapa" label="Asignación" sclass="mItem"
													visible="true" />
											</menupopup>
										</combobutton>
									</div>
								</row>
							</rows>
						</grid>
						<space />
						<space />
					</groupbox>
				</div>

				<div align="center">
					<div align="center" style="max-width: 950px;">
						<listbox id="listEtapas" nonselectableTags="*"
							oddRowSclass="none"
							emptyMessage="No se han encontrado resultados para esta búsqueda">
							<listhead>

								<listheader align="left"
									label="Seleccione la(s) Etapa(s)" width="56%">

								</listheader>


								<listheader align="left"
									label="Descripción" width="56%">
									<checkbox />
								</listheader>


							</listhead>
							<template name="model">


								<listitem>

									<listcell></listcell>

									<listcell
										style="text-align:left;">
										<label
											if="${each.estatus=='Inactivo'}" style="color: red"
											value="${each.descripcion}" />
										<label
											if="${each.estatus=='Activo'}" value="${each.descripcion}" />
									</listcell>

								</listitem>


							</template>
						</listbox>
						<space></space>
						<space></space>
						<div id="divMenuEtapa" align="center"
							height="50px">
							<hlayout style="padding: 20px">
								<button sclass="btn-primary btn-mini"
									zclass="btn" label="Añadir" id="btnAnnadirEtapa" />
								<button sclass="btn-danger btn-mini"
									zclass="btn" label="Volver" id="btnVolverEtapa" />

							</hlayout>
						</div>
						<space></space>
						<space></space>
					</div>
				</div>
			</tabpanel>

		</tabpanels>

	</tabbox>







</div>